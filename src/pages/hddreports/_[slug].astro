---
//
//
// PAGE IS DISABLED DUE TO CHANGE FROM COLLECTION 
//           TYPE FROM "CONTENT" TO "DATA"
//
//
import { type CollectionEntry, getCollection } from 'astro:content';
import type { HddSchema } from '../../content/config';
import HddLayout from '@layouts/HddLayout.astro';
import dayjs from 'dayjs';
import localizedFormat from 'dayjs/plugin/localizedFormat';
dayjs.extend(localizedFormat);

 const hddEntries = await getCollection('hddreports');
 hddEntries.map(entry => ( { params: { slug: entry.id }, props: { entry }, } )); 
interface Props { entry: CollectionEntry<"hddreports">; }
const { entry } = Astro.props;
const post : HddSchema = entry.data;
---
<HddLayout 
    serial={post.serial}
    make={post.make}
    model={post.model}
    reportDate={post.reportDate}
    capacity={post.capacity}
    size={post.size}
    type={post.type}
    manuDate={post.manuDate}
    pdf={post.pdf}
    isSSD={post.isSSD}
    >
</HddLayout>